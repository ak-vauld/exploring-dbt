[flake8]
select = ANN,B,B9,BLK,C,D,DAR,E,F,I,S,W
ignore = E203,E501,W503,B903
exclude=.venv,.git,__pycache__,notebooks,docs,build,dist
max-line-length = 88
max-complexity = 10
application-import-names = test
import-order-style = google
docstring-convention = google
per-file-ignores = tests/*:S101,ANN
